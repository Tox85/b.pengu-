{
  "name": "b.pengu",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:unit": "cross-env ENABLE_CEX=false vitest run -c vitest.unit.config.ts --reporter=verbose",
    "test:int": "cross-env ENABLE_CEX=false DRY_RUN=true vitest run -c vitest.int.config.ts --reporter=verbose",
    "test:e2e-dry": "cross-env ENABLE_CEX=false DRY_RUN=true vitest run --runInBand -c vitest.e2e.config.ts --reporter=verbose",
    "coverage": "vitest run --coverage",
    "run-all:dry": "cross-env ENABLE_CEX=false DRY_RUN=true ts-node src/cli/runAll.ts",
    "deadcode:ts": "ts-prune -p tsconfig.json",
    "deadcode:deps": "knip",
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/main.ts",
    "start": "node dist/main.js",
    "start:simulation": "node scripts/start-simulation.js",
    "start:dry-run": "cross-env ENABLE_CEX=false DRY_RUN=true node scripts/start-dry-run.js",
    "start:micro": "node scripts/start-micro-amounts.js",
    "run-all": "cross-env ENABLE_CEX=true DRY_RUN=false ts-node src/main.ts",
    "docs:runbook": "node -e \"console.log('Voir docs/RUNBOOK.md')\"",
    "docs:legacy": "node -e \"console.log('Voir docs/LEGACY_MAP.md')\"",
    "pretest:guard": "npx ts-prune && node scripts/check-no-legacy-imports.js || true",
    "lp:base:dry": "cross-env DRY_RUN=true npx tsx scripts/base-only-lp.ts",
    "lp:base:live": "cross-env DRY_RUN=false npx tsx scripts/base-only-lp.ts",
    "e2e:bridge:dry": "cross-env DRY_RUN=true npx tsx scripts/e2e-bridge-solana.ts",
    "e2e:bridge:live": "cross-env DRY_RUN=false npx tsx scripts/e2e-bridge-solana.ts",
    "test:e2e:bridge": "vitest run tests/e2e.bridge.dry.spec.ts --reporter=verbose",
    "test:e2e:solana": "vitest run tests/e2e.solana.swap-lp.dry.spec.ts --reporter=verbose",
    "test:e2e:live": "vitest run tests/e2e.full.live.spec.ts --reporter=verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bip39": "^2.4.2",
    "@types/jest": "^30.0.0",
    "@types/nock": "^10.0.3",
    "@types/node": "^24.5.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "jest": "^30.1.3",
    "knip": "^5.63.1",
    "nock": "^14.0.10",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.1",
    "ts-jest": "^29.4.2",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "@solana/web3.js": "1.87.0",
    "rpc-websockets": "7.4.2",
    "ws": "8.13.0"
  },
  "dependencies": {
    "@coral-xyz/anchor": "0.26.0",
    "@jup-ag/api": "^6.0.44",
    "@orca-so/whirlpools-sdk": "0.9.0",
    "@project-serum/anchor": "0.26.0",
    "@solana/spl-token": "0.3.11",
    "@solana/spl-token-registry": "^0.2.457",
    "@solana/web3.js": "1.87.0",
    "axios": "^1.12.2",
    "better-sqlite3": "^9.2.2",
    "bip39": "^3.1.0",
    "bn.js": "5.2.1",
    "ccxt": "^4.5.4",
    "cross-fetch": "4.0.0",
    "dotenv": "16.4.5",
    "ethers": "^6.15.0",
    "node-cron": "^4.2.1",
    "p-retry": "6.2.0",
    "pino": "^9.9.5",
    "zod": "^4.1.8"
  }
}
